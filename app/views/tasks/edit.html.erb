<% @lists = current_user.lists.all %>
<h1 class="text-center mt-2">Editing Task</h1>
<div class="container">
  <div class="row">
    <div class="col-12">
      <%= render 'form', task: @task %>
      <%#= link_to 'Show', @task %>
      <div class="row justify-content-center mt-5">
        <div class="col text-center">
          <button class="btn btn-secondary">
            <%= link_to 'Back', :back, :class => "btn", :style=> 'color:#FFFFFF;'%>
          </button>
        </div>
      </div> 
    </div>  
  </div>
</div>

